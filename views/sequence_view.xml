<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
		<record id="seq_new_inquiry_id" model="ir.sequence">
			<field name="name">New Inquiry</field>
			<field name="code">seq.new.inquiry</field>
			<field name="prefix">INQ-</field>
			<field name="padding">6</field>
		</record>

		<record id="seq_client_query_id" model="ir.sequence">
			<field name="name">Client Query</field>
			<field name="code">sr.no</field>
			<field name="prefix">CQ-</field>
			<field name="padding">5</field>
		</record>

		<record id="seq_revision_request_inquiry_id" model="ir.sequence">
			<field name="name">RR Inquiry</field>
			<field name="code">seq.revision.request.inquiry</field>
			<field name="prefix">RR-</field>
			<field name="padding">6</field>
		</record>

		<!--FOR INDIVIDUAL INVOICING SEQUENCE-->
		<record id="seq_individual_invoice_id" model="ir.sequence">
			<field name="name">Individual Invoice</field>
			<field name="code">seq.individual.invoice</field>
			<field name="prefix">IW%(year)s%(month)s%(day)s-</field>
			<field name="padding">2</field>
		</record>

		<!--FOR MONTHLY INVOICING SEQUENCE-->
		<record id="seq_monthly_invoice_id" model="ir.sequence">
			<field name="name">Monthly Invoice</field>
			<field name="code">seq.monthly.invoice</field>
			<field name="prefix">MW%(year)s%(month)s-</field>
			<field name="padding">2</field>
		</record>

	</data>
</odoo>